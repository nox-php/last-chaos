{
	"name": "nox-php/last-chaos",
	"description": "Add LastChaos game integration to your website.",
	"keywords": [
		"nox",
		"last-chaos",
		"nox-module"
	],
	"require": {
		"php": "^8.1",
		"nox-php/framework-new": ">=0.0.9.6"
	},
	"autoload": {
		"psr-4": {
			"Nox\\LastChaos\\": "src"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Nox\\LastChaos\\LastChaosServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
